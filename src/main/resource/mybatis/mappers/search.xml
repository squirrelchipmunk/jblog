<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="search">
	
	
	<select id="selectList" parameterType="String" resultType="SearchVo">
		<![CDATA[
			select logoFile,
			       blogTitle,
			       u.userName,
			       u.id,
			       u.joinDate
			from users u, blog b
			where u.id = b.id and
			      blogTitle like '%ë¸”%'	
		]]>
	</select>
	

</mapper>